<AnchorPane stylesheets="@CSS/driverStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.DriverLayoutController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <GridPane fx:id="clockGridLayout" alignment="CENTER" hgap="6.0" vgap="6.0">
              <columnConstraints>
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
              </rowConstraints>
            </GridPane>
         </center>
         <top>
            <JFXToolbar BorderPane.alignment="CENTER">
               <center>
                  <JFXButton onAction="#startAction" styleClass="primary-button-1" text="Start" BorderPane.alignment="CENTER" />
               </center>
               <padding>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </padding>
            </JFXToolbar>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
